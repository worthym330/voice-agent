# Environment Variables for Voice Agent
# Copy this file to .env and fill in your actual API keys

# Twilio Configuration
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=+1xxxxxxxxxx
TARGET_PHONE_NUMBER=+919004523829

# Public URL - CRITICAL: Replace with your ngrok URL
# Get this from running: ngrok http 8000
# Example: https://abc123.ngrok.io
PUBLIC_URL=https://your-ngrok-url.ngrok.io

# Google Gemini API Key (for intelligent conversation)
GEMINI_API_KEY=your_gemini_api_key_here

# ElevenLabs API Key (for text-to-speech)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Optional: API Key for securing endpoints
# API_KEY=your_secret_api_key

# Optional: Server Port (default: 8000)
# PORT=8000
